  "main": "dist/index.js",
  "scripts": {
    "init": "npm i && npm run fmt",
    "prebuild": "npx rimraf dist && npm run lint",
    "postbuild": "npx copyfiles src/**/*.json dist/ -u 1",
    "build": "npx tsc --project tsconfig.json",
    "lint": "npx tslint --project tsconfig.json",
    "fmt": "npm run lint -- --fix",
    "dev": "ts-node-dev --respawn --files src/index --pretty | pino-pretty -c",
    "command:test": "ts-node --files src/command Test",
    "test": "npx jest --forceExit --no-cache --runInBand --detectOpenHandles --config src/jest.config.js",
    "start": "node -r source-map-support/register dist/index",
    "start:pm2": "pm2 start dist/index.js"
  },
